{
  "redis_host": "$REDIS_HOST",
  "redis_port": 6379,
  "redis_auth": null,
  "http_host": "0.0.0.0",
  "http_port": 7379,
  "threads": 5,
  "pool_size": 20,
  "daemonize": false,
  "websockets": false,
  "database": 0,
  "acl": [
    {
      "disabled": ["*"]
    },
    {
      "http_basic_auth": "username:passwd",
      "enabled": ["*"]
    }
  ],
  "verbosity": 4,
  "logfile": "/dev/stdout"
}